<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Cars</title>
</head>
<body>
    <h1>Cars List</h1>
    <a href="/logout">Logout</a>
    <table>
        <thead>
            <tr>
                <th>Car Name</th>
                <th>Year</th>
                <th>Price</th>
            </tr>
        </thead>
        <tbody>
            <% cars.forEach(car => { %>
            <tr>
                <td><%= car.name %></td>
                <td><%= car.year %></td>
                <td><%= car.price %></td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
